@model IEnumerable<SportsManagement.Models.competitor>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model.Competitor_Name)</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Competitor_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Competitor_ID }) |
            @Html.ActionLink("Details", "Details", new { id = item.Competitor_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Competitor_ID })
        </td>
    </tr>
}

</table>
